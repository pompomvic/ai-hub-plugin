{
  "name": "ai-hub/wp-seo-plugin",
  "description": "AI Hub multi-tenant WordPress integration plugin for synchronising SEO drafts.",
  "type": "wordpress-plugin",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.9",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "wp-coding-standards/wpcs": "^3"
  },
  "autoload": {
    "psr-4": {
      "AIHub\\\\WordPress\\\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AIHub\\\\WordPress\\\\Tests\\\\": "tests/"
    }
  },
  "scripts": {
    "lint": "phpcs --standard=phpcs.xml.dist",
    "lint:fix": "phpcbf --standard=phpcs.xml.dist",
    "test": "phpunit --configuration phpunit.xml.dist"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
